<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Incident Reporting</title>
    @*css*@
    <link href="~/shared/styles/base.min.css" rel="stylesheet" />
    <link href="~/lib/kendo/css/web/kendo.default-v2.min.css" rel="stylesheet" />
    <link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />

    @*java script*@
    <script src="~/js/jquery.min.js"></script>
    <script src="~/js/kendo.all.min.js"></script>
    <script src="~/js/kendo.aspnetmvc.min.js"></script>
    <script src="~/lib/kendo/js/cultures/kendo.culture.de-DE.min.js"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-md navbar-toggleable-sm navbar-dark mb-3" style="background-color: #5d89b4;">
            <div class="container  col-md-12">
                <a class="navbar-brand" asp-area="">
                    <img src="~/logo/Exp Logo - White (PNG).png" style="height: 25px;" />
                    <span class="text-white" style="margin-left: 0.5em">Incident Reporting</span>
                </a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container-fluid">
      
            @RenderBody()
      
    </div>
    <footer class="border-top text-muted" style="text-align: center">
        <span>Copyright &copy; 2020</span>
        <img src="~/logo/Exp Logo - Color - CMYK (PNG).png" style="height: 25px" />
    </footer>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>*@
    @RenderSection("Scripts", required: false)
</body>
</html>
